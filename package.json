{
    "title": "Personal",
    "name": "startbootstrap-personal",
    "version": "1.0.1",
    "scripts": {
        "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "A personal website template built with Bootstrap",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://devinreichenbach.github.io/",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/DevinReichenbach/DevinReichenbach.github.io"
    },
    "dependencies": {
        "bootstrap": "5.2.3",
        "bootstrap-icons": "^1.11.3",
        "clean.js": "^0.0.3"
    },
    "devDependencies": {
        "autoprefixer": "10.4.14",
        "browser-sync": "^3.0.4",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "gh-pages": "^6.3.0",
        "postcss": "^8.5.3",
        "prettier": "2.8.6",
        "pug": "^3.0.3",
        "sass": "1.60.0",
        "shelljs": "^0.8.5",
        "upath": "2.0.1"
    }
}
